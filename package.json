{
  "name": "icd-frontend-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:smoke": "jest tests/api/smoke.test.ts",
    "test:migration": "node test-migration-routes.js",
    "test:fallback": "node test-fallback-mechanism.js",
    "test:migration:json": "USE_DB=false node test-migration-routes.js",
    "test:migration:db": "USE_DB=true node test-migration-routes.js",
    "check:email": "tsx scripts/check-email-config.ts",
    "check:tables": "tsx scripts/check-tables.ts",
    "diagnose:user": "tsx scripts/diagnose-user-issue.ts",
    "diagnose:prestataires": "tsx scripts/diagnose-prestataire-ids.ts",
    "postinstall": "echo 'skip prisma generate on install'",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postbuild": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^7.1.0",
    "@types/pdfkit": "^0.17.4",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "date-fns-tz": "^3.2.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.555.0",
    "next": "16.0.10",
    "pdfkit": "^0.17.2",
    "prisma": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.6.0",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
